%Defining The Case Function%
\newcounter{casenum}
\newenvironment{caseof}{\setcounter{casenum}{1}}{\vskip.5\baselineskip}
\newcommand{\case}[2]{
    \begin{adjustwidth}{1em}{1em}
        \vskip.5\baselineskip\par\noindent {\bfseries Case \arabic{casenum}:} #1\\#2\addtocounter{casenum}{1}
    \end{adjustwidth}
}

%Defining the subproof environment%
\newenvironment{subproof}[1][\proofname]{%
  \renewcommand{\qedsymbol}{\ensuremath{\blacksquare}}%
  \begin{proof}[#1]%
}{%
  \end{proof}%
}

%Defining the subproof environment%
\newenvironment{proofc}[1][Proof (By Contradiction).]{%
  \begin{proof}[#1]%
}{%
  \end{proof}%
}